pico-8 cartridge // http://www.pico-8.com
version 8
__lua__
player = {
  sprite = 1,

  x = 64,
  y = 64,
  angle = 0,

  rect1 = {
    x = 0,
    y = -1,
  },

  rect2 = {
    x = 0,
    y = 5,
  },
}

cam = {
  x = 0,
  y = 0,
}

function _update()
  if btn(0) then -- left
    player.angle += 0.05
  end

  if btn(1) then -- right
    player.angle -= 0.05
  end

  if btn(2) then -- up
    player.x += cos(-(player.angle - 0.25)) -- this should be zero
    player.y += sin(player.angle - 0.25) -- this should be positive
  end

  if btn(3) then -- down
    player.x += cos(player.angle + 0.25) -- should be zero
    player.y += sin(player.angle + 0.25) -- should be negative
  end

  camera(cam.x, cam.y)
end

function _draw()
  cls()
  print(player.angle, 5, 80)
  --px, py = rotate(
   -- player.x, player.y, 64, 64, player.angle
  --)
  px, py = player.x, player.y


  x1, y1 = rotate(
    player.x + player.rect1.x,
    player.y + player.rect1.y,
    player.x,
    player.y,
    player.angle
  )
  
  x2, y2 = rotate(
    player.x + player.rect2.x,
    player.y + player.rect2.y,
    player.x,
    player.y,
    player.angle
  )

  --rectfill(x1, y1, x2, y2)
  line(x1, y1, x2, y2)

  circfill(px, py, 2)
  circfill(0, 0, 40)

--  --spr(player.sprite, player.x, player.y)
--  print(player.angle, 5, 5, 3)
end

-- take a 2D position (x, y), and rotate it by angle
-- (cx, cy) represents the pivot point
-- https://www.lexaloffle.com/bbs/?tid=29275
function rotate(x, y, cx, cy, angle)
  local sina = sin(angle)
  local cosa = cos(angle)

  -- local position
  x -= cx
  y -= cy

  -- rotation matrix
  local rotx = cosa*x - sina*y
  local roty = sina*x + cosa*y

  -- back to global position
  rotx += cx
  roty += cy

  return rotx, roty
end
